    "build": "cross-env NODE_ENV=prod  webpack --config ./webpack.config.js",
    "ser": "node ./server/index.js",
    "cle:dep": "rm -rf node_modules && rm yarn.lock",
    "pre:commit": "echo pre:commit-hook",
    "pre:push": "echo pre:push-hook"
  },
